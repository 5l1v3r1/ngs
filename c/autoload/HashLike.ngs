{
	type HashLike

	F len(hl:HashLike) hl.(hl.hash_like_attr).len()
	F '[]'(hl:HashLike, k) hl.(hl.hash_like_attr)[k]
	F '[]='(hl:HashLike, k, v) hl.(hl.hash_like_attr)[k] = v

	F each(hl:HashLike, cb:Fun) hl.(hl.hash_like_attr).each(cb)
	# TODO: mapk, mapv and friends
	# TOOD: Hash() - copy or hash attribute?

	F init(hl:HashLike, attr:Str='items') {
		hl.hash_like_attr = attr
		hl.(hl.hash_like_attr) = {}
	}

}

