{
	h = {
		"xyz": 10
		"xyw": 11
		"xy1": 12
		"xy2": 13
	}

	echo("Test: in")
	dump("xyz" in h)
	dump("xyz1" in h)
	dump("1xyz" in h)

	echo("Test: hash() is int")
	dump(hash("xyz") is Int)

	echo("Test: keys order")
	dump(keys(h))

	echo("Test: values order")
	dump(values(h))

	echo("Test: replace value")
	dump(h["xyw"])
	h["xyw"] = 1122;
	dump(h["xyw"])

	echo("Test: len")
	dump(len(h))
}
