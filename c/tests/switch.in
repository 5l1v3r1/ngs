echo('switch')
dump(switch 1 { 1 10 2 20 })
dump(switch 2 { 1 10 2 20 })
dump(switch 3 { 1 10 2 20 })
echo('eswitch')
dump(try eswitch 1 { 1 10 2 20 } catch(e:SwitchFail) "OK")
dump(try eswitch 2 { 1 10 2 20 } catch(e:SwitchFail) "OK")
dump(try eswitch 3 { 1 10 2 20 } catch(e:SwitchFail) "OK")

F match(a:Int, b:Int) a>b
echo('match')
dump(match 1 { 1 10 2 20 })
dump(match 2 { 1 10 2 20 })
dump(match 3 { 1 10 2 20 })
echo('ematch')
dump(try ematch 1 { 1 10 2 20 } catch(e:SwitchFail) "OK")
dump(try ematch 2 { 1 10 2 20 } catch(e:SwitchFail) "OK")
dump(try ematch 3 { 1 10 2 20 } catch(e:SwitchFail) "OK")

echo('cond')
dump(cond { true  10 false 20 })
dump(cond { false 10 true  20 })
dump(cond { false 10 false 20 })
echo('econd')
dump(try econd { true  10 false 20 } catch(e:SwitchFail) "OK")
dump(try econd { false 10 true  20 } catch(e:SwitchFail) "OK")
dump(try econd { false 10 false 20 } catch(e:SwitchFail) "OK")
